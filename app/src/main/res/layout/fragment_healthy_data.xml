<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <android.support.design.widget.TabLayout
        android:id="@+id/tabHealthyData"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorTab"
        app:tabIndicatorColor="@color/colorYellow1"
        app:tabTextAppearance="@style/MyTabLayoutTextAppearance"></android.support.design.widget.TabLayout>

    <com.zty.healthy.healthyplus.widget.MyViewPager
        android:id="@+id/viewPagerHealthyData"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></com.zty.healthy.healthyplus.widget.MyViewPager>


</LinearLayout>